<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IRegisterLogin" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_ILudo" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/RegisterLoginService/RegisterLogin/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRegisterLogin"
                contract="RegisterLoginServiceReference.IRegisterLogin" name="BasicHttpBinding_IRegisterLogin" />
            <endpoint address="net.tcp://localhost:8734/Ludo" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_ILudo" contract="ILudoServiceReference.ILudo"
                name="NetTcpBinding_ILudo">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>